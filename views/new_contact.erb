<h2>Create New Contact</h2>

<form action="/contacts" method="post">        <!-- verbs supported by form: post,get-->
  <strong>First Name:</strong> <input type="text" name="first_name" autofocus placeholder="yo first name"> 
  <strong>Last Name:</strong> <input type="text" name="last_name" required="required" placeholder="yo last name">
  <strong>E-mail Address:</strong> <input type="text" name="email" placeholder="e-mail">
  <strong>Notes:</strong> <input type="text" name="notes" placeholder="notes">
  
  <input type="submit" name="Submit" value="Save Contact">
</form>
